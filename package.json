  },
  "devDependencies": {
    "@types/next": "^7.0.5",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-native": "^0.55.28",
    "@zeit/next-typescript": "^1.1.1",
    "next-plugin-transpile-modules": "^0.1.3",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1"
