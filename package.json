{
  "name": "este",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "dev:api": "nodemon --config nodemon.api.json api/index.tsx",
    "dev:web": "next",
    "dev": "run-p dev:api dev:web",
    "build:api": "rm -rf api-production && tsc --project tsconfig.api.json",
    "build:web": "next build",
    "build": "run-p build:api build:web",
    "start:api": "NODE_ENV=production node -r esm -r ts-node/register/transpile-only api/index.tsx",
    "start:web": "next start",
    "start": "run-p start:api start:web",
    "test:lint": "tslint -p .",
    "test:tsc": "tsc --project tsconfig.json",
    "test": "run-s test:lint test:tsc",
    "deps": "rm -rf package-lock.json node_modules && npm i"
  },
  "dependencies": {
    "esm": "^3.0.84",
    "next": "^7.0.2",
    "react": "^16.7.0-alpha.2",
    "react-art": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-intl": "^2.7.2",
    "react-native-web": "^0.9.8",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@types/next": "^7.0.5",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-intl": "^2.3.14",
    "@types/react-native": "^0.55.28",
    "@types/validator": "^9.4.3",
    "@zeit/next-typescript": "^1.1.1",
    "next-plugin-transpile-modules": "^1.0.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1"
  }
}
