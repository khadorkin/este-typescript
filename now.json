{
  "version": 2,
  "name": "este",
  "builds": [
    { "src": "api-production/index.jsx", "use": "@now/node" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/api/(.*)", "dest": "/api-production/index.jsx" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
